    <property name="title" translatable="yes">About Junqer</property>
    <property name="type_hint">normal</property>
    <property name="program_name">Junqer</property>
    <property name="version">0.1</property>
    <property name="copyright" translatable="yes">mru	</property>
    <property name="comments" translatable="yes">show junking make easy</property>
    <property name="website">http://sisyphus.teil.cc/~mru</property>
    <property name="authors">mru &lt;mru@sisyphus.teil.cc&gt;</property>
    <child internal-child="vbox">
